{
  "name": "automatikdocs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run automatikdocs:serve-ssr",
    "serve:ssr": "node ../server/public/automatikdocs/server/main.js",
    "build:ssr": "ng build --prod && ng run automatikdocs:server:production",
    "watch:ssr": "nodemon --watch src/ -e ts,html,scss --exec 'npm run build:ssr'",
    "prerender": "ng run automatikdocs:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
