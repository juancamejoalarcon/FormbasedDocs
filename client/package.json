    "@nguniversal/builders": "^9.1.0",
    "@types/braintree-web": "^3.47.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "blob-polyfill": "^4.0.20190430",
    "braintree-web": "^3.60.0",
    "braintree-web-drop-in": "^1.22.1",
    "codelyzer": "^5.1.2",
    "gulp": "^4.0.2",
    "gulp-purifycss": "^0.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.4.1",
